$(document).ready(function(){
  $('.calender').datepicker({
    dateFormat: "yy-mm-dd"
  });

  //$('')
  $('.show-smiles').toggle(function(){
    $(this).text('Hide Smiles');
    $('.smiles-table').show();
  }, function(){
    $(this).text('Show Smiles');
    $('.smiles-table').hide();
  });

  $('.smiles-table').hide();
})

